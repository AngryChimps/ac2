<?php
        $structure['namespace'] = '{{namespace}}';
        $structure['names'] = [{{#each tables}}'{{name}}', {{/each}}];

    {{#each datastores}}
        $structure['datastores']['{{name}}']['name'] = '{{name}}';
        $structure['datastores']['{{name}}']['driver'] = '{{driver}}';
    {{#if isRiak2}}
        $structure['datastores']['{{name}}']['prefix'] = '{{prefix}}';
    {{/if}}
    {{#if isElasticsearch}}
        $structure['datastores']['{{name}}']['default_analyzer'] = '{{default_analyzer}}';
        $structure['datastores']['{{name}}']['shards'] = '{{shards}}';
        $structure['datastores']['{{name}}']['replicas'] = '{{replicas}}';
    {{/if}}
    {{/each}}

    {{#each entities}}
        $structure['entities']['{{name}}']['name'] = '{{name}}';
        $structure['entities']['{{name}}']['objectName'] = '{{objectName}}';
        $structure['entities']['{{name}}']['collectionName'] = '{{objectName}}Collection';
        $structure['entities']['{{name}}']['primaryDatastoreName'] = '{{primaryDatastore.name}}';
        $structure['entities']['{{name}}']['secondaryDatastoreNames'] = {{{secondayDatastoreNamesString}}};

        $structure['entities']['{{name}}']['primaryKeyFieldNames'] = {{{primaryKeyFieldNamesString}}};
        $structure['entities']['{{name}}']['primaryKeyPropertyNames'] = {{{primaryKeyPropertyNamesString}}};
        $structure['entities']['{{name}}']['autoIncrementField'] = '{{autoIncrementField}}';
        $structure['entities']['{{name}}']['autoIncrementProperty'] = '{{autoIncrementProperty}}';
        $structure['entities']['{{name}}']['autoGenerateField'] = '{{autoGenerateField}}';
        $structure['entities']['{{name}}']['autoGenerateProperty'] = '{{autoGenerateProperty}}';
        $structure['entities']['{{name}}']['fieldNames'] = {{{fieldNamesString}}};
        $structure['entities']['{{name}}']['propertyNames'] = {{{propertyNamesString}}};
        $structure['entities']['{{name}}']['fieldTypes'] = {{{fieldTypesString}}};
        $structure['entities']['{{name}}']['apiPublicFields'] = {{{apiPublicFieldsString}}};
        $structure['entities']['{{name}}']['apiPrivateFields'] = {{{apiPrivateFieldsString}}};
        $structure['entities']['{{name}}']['apiHiddenButSettableFields'] = {{{apiHiddenButSettableFields}}};

        $structure['classes']['{{objectName}}']['name'] = '{{name}}';
        $structure['classes']['{{objectName}}']['primaryDatastoreName'] = '{{primaryDatastoreName}}';
        $structure['classes']['{{objectName}}']['secondaryDatastoreNames'] = {{{secondayDatastoreNamesString}}};
        $structure['classes']['{{objectName}}Collection']['name'] = '{{name}}';
        $structure['classes']['{{objectName}}Collection']['primaryDatastoreName'] = '{{primaryDatastoreName}}';
        $structure['classes']['{{objectName}}Collection']['secondaryDatastoreNames'] = {{{secondayDatastoreNamesString}}};

    {{/each}}

    {{#each subclasses}}
        $structure['subclasses']['{{name}}']['name'] = '{{name}}';
        $structure['subclasses']['{{name}}']['objectName'] = '{{objectName}}';
        $structure['classes']['{{objectName}}']['name'] = '{{name}}';

    {{/each}}
